{
  "name": "los-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "bash scripts/cleanup-port.sh && vite",
    "dev:tauri": "bash scripts/cleanup-port.sh && npm run tauri dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "clean:port": "bash scripts/cleanup-port.sh",
    "clean:all": "bash scripts/cleanup-port.sh && rm -rf dist node_modules/.vite",
    "validate:hmr": "echo 'Testing HMR...' && echo 'Make a change to any .tsx file to test hot reload'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx src",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@types/uuid": "^10.0.0",
    "@xenova/transformers": "^2.17.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "idb": "^8.0.3",
    "lucide-react": "^0.544.0",
    "pdfjs-dist": "^5.4.296",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.3",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.4.18",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
